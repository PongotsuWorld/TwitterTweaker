{
  "name": "TwitterTweaker",
  "version": "2025.8.3",
  "manifest_version": 3,
  "description": "Twitterの拡張",
  "content_scripts": [{
    "matches": ["https://x.com/*"],
    "css": ["index.css"]
  }],
  "permissions": ["tabs"],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-48.png"
  }
}